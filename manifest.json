{
  "manifest_version": 2,
  "name": "adb",
  "description": "adblcker",
  "version": "1.0",
  "web_accessible_resources": [
    "images/blocked.svg"
  ],
  "background": {
    "scripts": ["blocker.js"]
  },

  "permissions": [
    "tabs",
    "<all_urls>",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "storage",
    "unlimitedStorage",
    "notifications"
  ],
  "optional_permissions": [
    "history"
  ],
  "applications": {
    "gecko": {
      "strict_min_version": "55.0a2"
    }
  }
}
